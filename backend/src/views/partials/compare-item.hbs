{{#each this}}
<li class="cart__table-body" data-name={{_id}}>
          <div class="cart__table-data first-data" ><a id="productId"  ><img class="cart__table-img" src={{image}} /></a><p>{{title}}</p></div>
          <div id="quantityBtn" class="cart__table-data  cart__table-data--quantity">
         <div>
             <p>{{descr}}</p>
         </div>
         </div>
          <div id="productPrice" class="cart__table-data ">${{price}}</div>
          <button type="button" class="button cart-btn " id="addBtn" data-id="add" >Add to Cart</button>
          <div class="cart__table-data "><button id="removeProductBtn" data-name={{_id}} class="cart__table-btn">
            {{> svg-cross}}
          </button></div>
</li>
{{/each}}